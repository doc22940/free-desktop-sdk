kind: autotools

depends:
  - filename: base.bst
    type: build
  - filename: desktop/gdk-pixbuf.bst
  - filename: desktop/pango.bst
  - filename: desktop/cairo.bst
  - filename: desktop/libcroco.bst
  - filename: base/rust.bst
  - filename: base/cargo.bst
    type: build

variables:
  builddir: ''

config:
  configure-commands:
    (<):
      - |
        mkdir home
        cat <<EOF >home/config
        [source.crates-io]
        replace-with = "vendored-sources"
        [source.vendored-sources]
        directory = "${PWD}/crates"
        EOF

environment:
  CARGO_HOME: "%{build-root}/home"

public:
  bst:
    split-rules:
      devel:
        (>):
          - "%{libdir}/librsvg-2.so"

sources:
  - kind: git
    url: https://github.com/GNOME/librsvg.git
    ref: ca640d1185745665c61689b71c627e1d020a80b7 # 2.42.3
    track: librsvg-2.42

